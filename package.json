{
  "homepage": "http://noby88.github.io/noby-ui-kit",
  "name": "noby-ui-kit",
  "version": "0.1.28",
  "keywords": [
    "react",
    "components",
    "noby",
    "ui",
    "ui-kit",
    "typescript",
    "theme context provider"
  ],
  "description": "An accessible React UI kit",
  "author": "noby88",
  "private": false,
  "main": "core/index.js",
  "module": "core/index.m.js",
  "types": "core/index.d.ts",
  "files": [
    "core",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/noby88/noby-ui-kit.git"
  },
  "dependencies": {
    "react": "^17.0.2",
    "styled-components": "^5.3.1"
  },
  "scripts": {
    "tmp": "rollup -c",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build-lib": "rm -rf core && tsc -p tsconfig-build-lib.json",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "do-all": "rm -rf core && tsc -p tsconfig-build-lib.json && npm run build && gh-pages -d build && npm publish"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.15.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/jest": "^26.0.24",
    "@types/node": "^12.20.21",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "@types/styled-components": "^5.1.13",
    "gh-pages": "^3.2.3",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "rollup": "^2.62.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-typescript2": "^0.31.1",
    "typescript": "^4.4.2",
    "web-vitals": "^1.1.2"
  }
}